<script>
    $(document).ready(function () {
        // Bắt sự kiện khi thẻ a được nhấp
        $('a').click(function (event) {
            // Kiểm tra xem href của thẻ a có tồn tại hay không
            var href = $(this).attr('href');
            if (href && href !== '#') {
                // Xóa cookie 'cake'
                document.cookie = "cake=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
                // Load lại trang
                window.location.href = href
                // Ngăn chặn hành động mặc định của thẻ a
                event.preventDefault();
            }
        });
    });
</script>